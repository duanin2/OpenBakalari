{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "default",
    "description": "Capability for the main window",
    "windows": [
	"main"
    ],
    "permissions": [
	"core:default",
	"shell:allow-open",
	{
	    "identifier": "http:default",
	    "allow": [
		{
		    "url": "https://*/api/login"
		},
		{
		    "url": "https://*/api/3/*"
		},
		{
		    "url": "http://*/api/login"
		},
		{
		    "url": "http://*/api/3/*"
		}
	    ],
	    "deny": []
	},
	"store:default"
    ]
}
